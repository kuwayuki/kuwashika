{
  "expo": {
    "plugins": [
      "expo-tracking-transparency",
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "speechRecogntionPermission": "Allow $(PRODUCT_NAME) to securely recognize user speech"
        }
      ]
    ],
    "name": "ぺリオチャート",
    "slug": "PerioChart",
    "version": "1.0.1",
    "orientation": "landscape",
    "icon": "./assets/images/icon.png",
    "scheme": "periochart",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-2103807205659646~3739470895"
      },
      "infoPlist": {
        "NSUserTrackingUsageDescription": "「許可」を押すと、他社のデータも活用し、よりお客様に適した広告が表示されます。",
        "NSMicrophoneUsageDescription": "This app uses the microphone to allow users to send orders via voice message.",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork"
          }
        ]
      },
      "bundleIdentifier": "com.kuwahara.ExpoExample"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-android.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-2103807205659646~2121866348"
      },
      "permissions": [],
      "package": "com.kuwahara.ExpoExample",
      "versionCode": 6
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "description": ""
  }
}
