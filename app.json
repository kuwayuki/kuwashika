{
  "expo": {
    "plugins": ["expo-tracking-transparency"],
    "name": "ぺリオチャート",
    "slug": "PerioChart",
    "version": "1.0.4",
    "orientation": "landscape",
    "icon": "./assets/images/icon.png",
    "scheme": "periochart",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-2103807205659646~3739470895"
      },
      "infoPlist": {
        "NSUserTrackingUsageDescription": "「許可」を押すと、他社のデータも活用し、よりお客様に適した広告が表示されます。",
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork"
          }
        ]
      },
      "bundleIdentifier": "com.kuwahara.ExpoExample"
    },
    "android": {
      "package": "ぺリオチャート",
      "googleMobileAdsAppId": "ca-app-pub-2103807205659646~2121866348",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "description": ""
  }
}
